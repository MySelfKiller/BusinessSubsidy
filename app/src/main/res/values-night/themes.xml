<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TeamSubsidy" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryVariant">@color/slight_white2</item>
        <item name="colorOnPrimary">@color/slight_white2</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/slight_white2</item>
        <item name="colorSecondaryVariant">@color/slight_white2</item>
        <item name="colorOnSecondary">@color/slight_white2</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/alpha_9</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/alpha_9</item>
        <item name="android:navigationBarColor">@color/alpha_9</item>
    </style>
</resources>